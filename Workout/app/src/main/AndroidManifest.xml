<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.exeter.workout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".stopwatchActivity"></activity>
        <activity android:name=".timerActivity"></activity>
        <activity android:name=".infoActivity"></activity>
        <activity android:name=".DetailActivity"></activity>
        <activity android:name=".programActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".programCreateActivity"></activity>
        <activity android:name=".programDetailActivity"></activity>
        <activity android:name=".statisticsActivity"></activity>
    </application>

</manifest>